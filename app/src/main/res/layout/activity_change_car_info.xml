<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/back"
                android:background="@color/white"
                android:id="@+id/change_back"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="车辆信息"
                android:layout_marginBottom="0dp"
                android:layout_marginLeft="40dp"
                android:textSize="30dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/little_grey">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="基本信息"
                    android:textColor="@color/more_grey"
                    android:layout_marginLeft="10dp"
                    android:textSize="15dp"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/item1_recycleview"
                    android:layout_marginTop="15dp"
                    android:padding="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="车牌号码"
                            android:textColor="@color/black"
                            android:textSize="15dp"/>

                        <Button
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="10dp"
                            android:textColor="@color/white"
                            android:text="浙"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button_blue"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:textColor="@color/black"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:textColor="@color/black"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:textColor="@color/black"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:textColor="@color/black"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:textColor="@color/black"/>
                        <EditText
                            android:layout_width="35dp"
                            android:layout_height="50dp"
                            android:textSize="20dp"
                            android:layout_marginLeft="8dp"
                            android:gravity="center"
                            android:background="@drawable/number_choose_button"
                            android:hint="新"
                            android:textColorHint="@color/grey"
                            android:textColor="@color/black"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="车辆类型"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            android:layout_marginLeft="5dp"/>

                        <Space
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="中型货车"
                            android:layout_gravity="center"
                            android:textColor="@color/more_grey"
                            android:textSize="18dp"/>

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:layout_gravity="center"
                            android:src="@mipmap/enter"
                            android:layout_marginRight="15dp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="动力类型"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            android:layout_marginLeft="5dp"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            >


                            <Button
                                android:layout_width="50dp"
                                android:layout_height="30dp"
                                android:textSize="15dp"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:text="汽油"
                                android:gravity="center"
                                android:background="@drawable/number_choose_button"/>

                            <Button
                                android:layout_width="50dp"
                                android:layout_height="30dp"
                                android:textSize="15dp"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:text="柴油"
                                android:gravity="center"
                                android:background="@drawable/number_choose_button"/>

                            <Button
                                android:layout_width="65dp"
                                android:layout_height="30dp"
                                android:textSize="15dp"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:text="纯电动"
                                android:gravity="center"
                                android:background="@drawable/number_choose_button"/>

                            <Button
                                android:layout_width="85dp"
                                android:layout_height="30dp"
                                android:textSize="15dp"
                                android:layout_marginLeft="10dp"
                                android:textColor="@color/black"
                                android:text="插电混合"
                                android:gravity="center"
                                android:background="@drawable/number_choose_button"/>
                        </LinearLayout>
                    </LinearLayout>



                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="获取更多参数,为避免违章和绕路，请准确填写，小数点后两位可手动输入"
                    android:textColor="@color/more_grey"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"/>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/item1_recycleview"
                        android:padding="5dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="总重量"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>

                            <SeekBar
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1" />

                            <EditText
                                android:layout_width="60dp"
                                android:gravity="center"
                                android:layout_height="30dp"
                                android:text="18.00"
                                android:textColor="@color/black"
                                android:background="@drawable/number_choose_button"
                                android:textSize="15dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="吨"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"
                                />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="核定载重"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>

                            <SeekBar
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_gravity="center"/>

                            <EditText
                                android:layout_width="60dp"
                                android:gravity="center"
                                android:layout_height="30dp"
                                android:text="18.00"
                                android:textColor="@color/black"
                                android:background="@drawable/number_choose_button"
                                android:textSize="15dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="吨"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"
                                />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="车长"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>

                            <SeekBar
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_gravity="center"/>

                            <EditText
                                android:layout_width="60dp"
                                android:gravity="center"
                                android:layout_height="30dp"
                                android:text="5.20"
                                android:textColor="@color/black"
                                android:background="@drawable/number_choose_button"
                                android:textSize="15dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="米"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"
                                />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="车宽"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>

                            <SeekBar
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1" />

                            <EditText
                                android:layout_width="60dp"
                                android:gravity="center"
                                android:layout_height="30dp"
                                android:text="1.20"
                                android:textColor="@color/black"
                                android:background="@drawable/number_choose_button"
                                android:textSize="15dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="米"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"
                                />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="车高"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>

                            <SeekBar
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_gravity="center"/>

                            <EditText
                                android:layout_width="60dp"
                                android:gravity="center"
                                android:layout_height="30dp"
                                android:text="2.00"
                                android:textColor="@color/black"
                                android:background="@drawable/number_choose_button"
                                android:textSize="15dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="米"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"
                                />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="轴数"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                android:layout_marginLeft="5dp"/>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                >

                                <Button
                                    android:layout_width="50dp"
                                    android:layout_height="30dp"
                                    android:textSize="15dp"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:text="2轴"
                                    android:gravity="center"
                                    android:background="@drawable/number_choose_button"/>

                                <Button
                                    android:layout_width="50dp"
                                    android:layout_height="30dp"
                                    android:textSize="15dp"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:text="3轴"
                                    android:gravity="center"
                                    android:background="@drawable/number_choose_button"/>

                                <Button
                                    android:layout_width="50dp"
                                    android:layout_height="30dp"
                                    android:textSize="15dp"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:text="4轴"
                                    android:gravity="center"
                                    android:background="@drawable/number_choose_button"/>

                                <Button
                                    android:layout_width="50dp"
                                    android:layout_height="30dp"
                                    android:textSize="15dp"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:text="5轴"
                                    android:gravity="center"
                                    android:background="@drawable/number_choose_button"/>

                                <Button
                                    android:layout_width="90dp"
                                    android:layout_height="30dp"
                                    android:textSize="15dp"
                                    android:layout_marginLeft="10dp"
                                    android:textColor="@color/black"
                                    android:text="6轴及以上"
                                    android:gravity="center"
                                    android:background="@drawable/number_choose_button"/>

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="@drawable/retangle_register_button"
                android:text="完成"
                android:textColor="@color/white"
                android:textSize="20dp" />

        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/text_grey"
        android:id="@+id/car_info_select_area_change">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="5dp">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/car_info_jing"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="0dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="京"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_jin2"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="津"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_hu"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="沪"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_yu2"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="渝"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_ji2"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="冀"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_jin"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text=" 晋"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_liao"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="辽"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_ji"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="吉"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_hei"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="黑"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_su"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="苏"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/car_info_zhe"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="0dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="浙"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_wan"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="皖"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_min"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="闽"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_gan"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="赣"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_lu"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="鲁"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_yu"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="豫"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_e"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="鄂"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_xiang"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="湘"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

                <Button
                    android:id="@+id/car_info_yue"
                    android:layout_width="35dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/number_choose_button_white"
                    android:gravity="center"
                    android:text="粤"
